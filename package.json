{
  "name": "image-map-editor",
  "version": "1.0.0",
  "description": "Adds SVG drawing capability (rectangles, circles, ellipses and polygons) on top of your image to let you make image maps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "tsc": "./node_modules/typescript/bin/tsc",
    "build": "tsc",
    "prepare": "husky",
    "format": "prettier --write **/*.{ts,tsx}",
    "demo": "webpack serve --config ./examples/node/webpack.config.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "xstate": "^4.23.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc-to-markdown": "^7.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^4.8.2",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint:fix"
    ]
  }
}
