{
  "name": "@joggs1/image-mapper",
  "version": "1.1.2",
  "description": "Adds SVG drawing capability (rectangles, circles, ellipses and polygons) on top of your image to let you make image maps",
  "type": "module",
  "license": "MIT",
  "main": "./dist/image-mapper.umd.js",
  "module": "./dist/image-mapper.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/joggS1/image-mapper"
  },
  "types": "./dist/ts/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/image-mapper.es.js",
      "require": "./dist/image-mapper.umd.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "tsc && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "tsc": "./node_modules/typescript/bin/tsc",
    "build": "vite build",
    "prepare": "husky",
    "format": "prettier --write **/*.{ts,tsx}",
    "demo": "node ./startdemo.js"
  },
  "dependencies": {
    "xstate": "^4.23.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@changesets/cli": "^2.27.1",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc-to-markdown": "^7.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "csstype": "^3.1.3",
    "typescript": "^4.8.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format"
    ]
  },
  "keywords": [
    "img-mapper",
    "image-mapper",
    "img mapper",
    "image map",
    "image mapper",
    "image annotation",
    "link image area",
    "design collaboration",
    "react",
    "react image mapper",
    "react img mapper"
  ]
}
